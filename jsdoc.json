{
  "source": {
    "include": [
      "src/classes",
      "src/handlers",
      "src/client.js",
      "src/util.js"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/",
    "template": "node_modules/better-docs"
  }
}